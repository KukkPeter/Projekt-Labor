{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./main",
      "rootDir": "./electron",
      "resolveJsonModule": true
    },
    "include": [
      "electron/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
}